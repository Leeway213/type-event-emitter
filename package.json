{
  "name": "type-event-emitter",
  "version": "1.0.1",
  "description": "",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint src --fix --ext .ts,.tsx",
    "test": "jest --passWithNoTests",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "types": "tsc --emitDeclarationOnly",
    "build:esm": "tsc --outDir ./dist/esm",
    "build:prod": "rollup -c --environment PRODUCTION,CJS,IIFE",
    "build:dev": "rollup -c --environment DEVELOPMENT,CJS,IIFE",
    "prebuild": "npm run clean",
    "build": "npm run types && npm run build:esm && npm run build:prod",
    "serve": "rollup -c -w --environment DEVELOPMENT,SERVE,IIFE",
    "bundle": "rollup -c --environment PRODUCTION,IIFE",
    "doc:api": "esdoc",
    "postversion": "git push --tags"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "main": "dist/main/index.js",
  "module": "dist/esm/index.js",
  "browser": "dist/browser/index.js",
  "types": "dist/types/index.d.ts",
  "author": {
    "name": "leeway.zlw",
    "email": "leeway.zlw@alibaba-inc.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.9.0",
    "@babel/preset-env": "7.9.0",
    "@babel/preset-typescript": "7.9.0",
    "@rollup/plugin-json": "^4.0.3",
    "@types/jest": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "babel-jest": "^26.0.1",
    "esdoc": "^1.1.0",
    "esdoc-exclude-source-plugin": "^1.0.0",
    "esdoc-publish-markdown-plugin": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "esdoc-typescript-plugin": "^1.0.1",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "~25.5.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "2.3.3",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-glslify": "^1.2.0",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-version-injector": "^1.2.2",
    "ts-jest": "~25.5.1",
    "typescript": "3.8.3"
  }
}
